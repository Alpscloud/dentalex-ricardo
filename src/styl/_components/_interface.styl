.city
	display inline-flex
	align-items center
	justify-content flex-start
	border 1px solid #e8e8e8
	padding 10px
	text-transform uppercase
	font 20px $SemiBold
	color $accent-color
	min-width 280px
	border-radius 29.5px
	&__ic
		flex none
		width 50px
		height 50px
		display block
		background $accent-color url('../img/icons/svg/location.svg') 50% no-repeat / 17px 24px
		border-radius 17.5px
		margin-right 30px
		